<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

	<rule>
		<note>
			首页
		</note>
		<from>/index.html</from>
		<to>/index</to>
	</rule>
	<rule>
		<note>
			连接
		</note>
		<from>/front/list/([0-9]+).html</from>
		<to>/index?action=list&amp;modelId=$1</to>
	</rule>
	<rule>
		<note>
			连接
		</note>
		<from>/front/detail/([0-9]+)/([0-9]+).html</from>
		<to>/index?action=detail&amp;modelId=$1&amp;id=$2</to>
	</rule>
</urlrewrite>
